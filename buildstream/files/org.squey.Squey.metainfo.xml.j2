<component type="desktop-application">
  <id>org.squey.Squey</id>
  <developer id="org.squey">
    <name>Squey</name>
  </developer>
  <metadata_license>FSFAP</metadata_license>
  <project_license>MIT</project_license>
  <name>Squey</name>
  <summary>Explore your data</summary>

  <description>
    {{project_description}}
  </description>

  <categories>
    <category>Science</category>
    <category>Graphics</category>
    <category>Network</category>
    <category>Education</category>
    <category>Utility</category>
  </categories>

  <launchable type="desktop-id">org.squey.Squey.desktop</launchable>

  <screenshots>
    <screenshot type="default">
      <caption>Squey let's you import structured text files...</caption>
      <image>https://gitlab.com/squey/squey/-/raw/main/buildstream/files/screenshots/squey_screenshot.png</image>
    </screenshot>
    <screenshot>
      <caption>...Apache Parquet files...</caption>
      <image>https://gitlab.com/squey/squey/-/raw/main/buildstream/files/screenshots/squey_screenshot2.png</image>
    </screenshot>
    <screenshot>
      <caption>...PCAP files, and more !</caption>
      <image>https://gitlab.com/squey/squey/-/raw/main/buildstream/files/screenshots/squey_screenshot3.png</image>
    </screenshot>
  </screenshots>

  <url type="homepage">https://squey.org</url>
  <url type="vcs-browser">https://gitlab.com/squey/squey</url>
  <url type="bugtracker">https://gitlab.com/squey/squey/-/issues</url>
  <url type="help">https://doc.squey.org</url>
  <url type="contribute">https://gitlab.com/squey/squey/-/blob/main/CONTRIBUTING.md</url>

  <project_group>SQUEY</project_group>

  <content_rating type="oars-1.1" />

  <releases>
    <release version="{{version}}" date="{{date}}">
        <description>
          {{changelog}}
        </description>
    </release>
  </releases>

</component>
