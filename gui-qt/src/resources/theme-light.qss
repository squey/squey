}

/* QToolBox --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qtoolbox

--------------------------------------------------------------------------- */
QToolBox {
  padding: 0px;
  border: 0px;
  border: 1px solid #dee0e2;
}

QToolBox:selected {
  padding: 0px;
  border: 2px solid #9fcbff;
}

QToolBox::tab {
  background-color: #fbfafd;
  border: 1px solid #dee0e2;
  color: #2a2e32;
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
}

QToolBox::tab:disabled {
  color: #a6aaad;
}

QToolBox::tab:selected {
  background-color: #acb1b6;
  border-bottom: 2px solid #9fcbff;
}

QToolBox::tab:selected:disabled {
  background-color: #dee0e2;
  border-bottom: 2px solid #daedff;
}

QToolBox::tab:!selected {
  background-color: #dee0e2;
  border-bottom: 2px solid #dee0e2;
}

QToolBox::tab:!selected:disabled {
  background-color: #fbfafd;
}

QToolBox::tab:hover {
  border-color: #58acfa;
  border-bottom: 2px solid #58acfa;
}

QToolBox QScrollArea QWidget QWidget {
  padding: 0px;
  border: 0px;
  background-color: #fbfafd;
}

