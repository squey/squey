/* (dot) .QFrame  fix #141, #126, #123 */
.QFrame {
  border-radius: 4px;
  border: 1px solid #dee0e2;
  /* No frame */
  /* HLine */
  /* HLine */
}

.QFrame[frameShape="0"] {
  border-radius: 4px;
  border: 1px transparent #dee0e2;
}

.QFrame[frameShape="4"] {
  max-height: 2px;
  border: none;
  background-color: #dee0e2;
}

.QFrame[frameShape="5"] {
  max-width: 2px;
  border: none;
  background-color: #dee0e2;
}

/* QSplitter --------------------------------------------------------------

